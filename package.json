{
  "name": "genie-router",
  "version": "2.0.0-beta",
  "description": "A generic platform that will route commands and conversations from voice or text-based clients to 3rd party backends.",
  "main": "router.js",
  "scripts": {
    "test": "eslint . && istanbul cover --include-all-sources true _mocha test/**/*.test.js --reporter=\"lcovonly html\" -- -R spec",
    "coverage-report": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "DEBUG=genie-router* ./bin/genie-router -c ./config.json",
    "nodemon": "DEBUG=genie-router* nodemon ./bin/genie-router -c ./config.json",
    "nodemon-test": "nodemon ./node_modules/.bin/mocha test/**/*.test.js",
    "standard-fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matueranet/genie-router.git"
  },
  "engines": {
    "node": ">=8.10"
  },
  "bin": {
    "genie-router": "bin/genie-router"
  },
  "keywords": [
    "genie",
    "genie-ai",
    "ai",
    "artifical",
    "intelligence",
    "router",
    "conversation",
    "commands",
    "hub"
  ],
  "author": "matueranet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matueranet/genie-router/issues"
  },
  "homepage": "https://github.com/matueranet/genie-router#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.2",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "npm-utils": "^2.0.0",
    "read-config": "^2.0.0",
    "superplug": "^0.2.0",
    "uuid": "^3.1.0",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.8.0",
    "sinon": "^4.2.2"
  }
}
