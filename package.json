{
  "name": "@genie-ai/genie-router",
  "version": "3.0.0",
  "description": "A generic platform that will route commands and conversations from voice or text-based clients to 3rd party backends.",
  "main": "router.js",
  "scripts": {
    "test": "eslint . && _mocha test/**/*.test.js",
    "start": "DEBUG=genie-router* ./bin/genie-router -c ./config.json",
    "nodemon": "DEBUG=genie-router* nodemon ./bin/genie-router -c ./config.json",
    "nodemon-test": "nodemon ./node_modules/.bin/mocha test/**/*.test.js",
    "eslint-fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genie-ai/genie-router.git"
  },
  "engines": {
    "node": ">=8.10"
  },
  "bin": {
    "genie-router": "bin/genie-router"
  },
  "keywords": [
    "genie",
    "genie-ai",
    "ai",
    "artifical",
    "intelligence",
    "router",
    "conversation",
    "commands",
    "hub"
  ],
  "author": "matueranet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/genie-ai/genie-router/issues"
  },
  "homepage": "https://github.com/genie-ai/genie-router#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.2",
    "debug": "^3.1.0",
    "express": "^4.15.3",
    "npm-utils": "^2.0.3",
    "read-config": "^2.0.0",
    "superplug": "^0.2.0",
    "uuid": "^3.1.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "proxyquire": "^2.0.1",
    "sinon": "^6.1.4"
  }
}
